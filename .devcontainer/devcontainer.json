{
	"name": "PySpark",
	"dockerComposeFile": "docker-compose.yml",
	"workspaceFolder": "/workspace",
	"service": "pyspark",
	"context": "..",
	"extensions": [
		"/extensions/databricks-vscode-0.6.0.vsix",
		"/extensions/eamodio.gitlens-11.0.6.vsix",
		"/extensions/ionutvmi.path-autocomplete-1.15.0.vsix",
		"/extensions/ms-python.python-2020.12.424452561.vsix",
		"/extensions/ms-toolsai.jupyter-2020.12.414227025.vsix",
		"/extensions/njpwerner.autodocstring-0.5.4.vsix",
		"/extensions/SonarSource.sonarlint-vscode-1.19.0.vsix"
	],
	"settings": {
		"python.pythonPath": "/usr/local/bin/python",
		"python.venvPath": "/.envs",
		"python.linting.banditEnabled": true,
		"python.linting.flake8Enabled": true,
		"python.testing.pytestEnabled": true,
		"python.linting.mypyEnabled": true,
		"editor.formatOnSave": true,
		"python.formatting.provider": "yapf",
		"autoDocstring.docstringFormat": "google"
	}
}